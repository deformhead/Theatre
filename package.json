{
    "author": "deformhead",
    "bugs": {

        "url": "https://github.com/deformhead/Theatre/issues"
    },
    "description": "javascript es6 game framework",
    "devDependencies": {

        "@babel/core": "7.4.0",
        "@babel/preset-env": "7.4.2",
        "babel-loader": "8.3.0",
        "electron": "5.0.8",
        "electron-packager": "14.0.3",
        "url-loader": "4.1.1",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "3.1.8",
        "webpack-merge": "4.1.4",
        "webpack-notifier": "1.6.0"
    },
    "homepage": "https://github.com/deformhead/Theatre#readme",
    "keywords": [

        "javascript",
        "es6",
        "game",
        "framework"
    ],
    "license": "MIT",
    "main": "sources/index.js",
    "repository": {

        "type": "git",
        "url": "git+https://github.com/deformhead/Theatre.git"
    },
    "scripts": {

        "build": "npm run build:linux:64 && npm run build:windows:32 && npm run build:windows:64",
        "build:linux:64": "electron-packager dist/ theatre.github.io --arch=x64 --asar --out=build/ --overwrite --platform=linux",
        "build:windows:32": "electron-packager dist/ theatre.github.io --arch=ia32 --asar --out=build/ --overwrite --platform=win32",
        "build:windows:64": "electron-packager dist/ theatre.github.io --arch=x64 --asar --out=build/ --overwrite --platform=win32",
        "dist": "webpack --config webpack.production.js",
        "electron": "electron dist/",
        "postinstall": "npm run dist",
        "start": "webpack-dev-server --config webpack.development.js",
        "test": "echo \"Hello, friend.\" && exit 0"
    }
}
